{
  "manifest_version": 2,
  "name": "Webcam Eye-Tracking Browwser Extension",
  "version": "0.9.0",
  "description": "This is a thesis study in MSU-IIT, Iligan City, Philippines. It is a Webcam Eye-Tracking Browser Extension for general navigation",
  "content_scripts": [
    {
      "matches": [
        "*://tictactoe.com.trigl-demo.com/*",
        "*://eye-tracking-look-to-speak-web.com.trigl-demo.com/*",
        "*://eye-tracking-food-menu.com.trigl-demo.com/*",
        "*://eye-tracking-youtube.com.trigl-demo.com/*"
      ],
      "js": [
        "js/global.js",
        "jquery.js",
        "webgazer.js",
        "overlay.js",
        "bookmark.js",
        "script.js",
        "js/chrome_scripts.js",
        "calibrate.js"
      ],
      "css": ["css/styles.css"]
    }
  ],
  "browser_action": {
    "default_title": "Eye Searcher",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "scripting"
  ]
}
