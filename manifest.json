{
  "manifest_version": 2,
  "name": "Eye Searcher",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/",
        "https://webgazer-calibration-thesis.herokuapp.com/calibration.html",
        "http://127.0.0.1:5500/tictactoe.html",
        "file:///C:/Users/chuck/Desktop/tictactoe/improvements/tictactoe.html",
        "*://tictactoe.com.trigl-demo.com/*",
        "http://localhost:8080/*",
        "http://localhost:8000/*",
        "*://eye-tracking-look-to-speak-web.com.trigl-demo.com/*",
        "*://eye-tracking-food-menu.com.trigl-demo.com/*",
        "*://eye-tracking-youtube.com.trigl-demo.com/*"
      ],
      "js": [
        "jquery.js",
        "webgazer.js",
        "overlay.js",
        "bookmark.js",
        "script.js",
        "js/chrome_scripts.js",
        "calibrate.js"
