{
  "manifest_version": 2,
  "name": "Eye Searcher",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webgazer.js", "script.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_title": "Eye Searcher",
    "default_popup": "popup.html"
  }
}
